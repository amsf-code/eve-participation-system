<div class="box">
  <div class="text-center">
    <div class="warning">
      <%= flash[:input_missing] %>
    </div>

    <%= form_for :alliance, url: alliance_index_path do |f| %>
      <p>
        <%= f.label :"alliance name" %><br>
        <%= f.text_field :name, class: "input-box" %>
      </p>

      <p>
        <%= f.submit :"add alliance"%>
      </p>
    <% end %>
  </div>
</div>
